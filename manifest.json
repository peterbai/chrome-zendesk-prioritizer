{
    "name": "Zendesk Prioritizer",
    "description": "Provides more advanced sorting of Zendesk tickets",
    "version": "0.0.4.0",
    "background": {
        "scripts": ["js/jquery-1.11.1.js", "js/background.js"]
    },
    "permissions": [
        "https://*.zendesk.com/",
        "storage",
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            "19": "icon/icon-19.png",
            "38": "icon/icon-38.png"
        },
        "default_title": "Zendesk Prioritizer",
        "default_popup": "html/popup.html"
    },
    "options_page": "html/options.html",
    "icons": {
        "128": "icon/icon-128.png"
    },
    "manifest_version": 2
}
